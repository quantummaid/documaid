<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <rules>
        <rule groupId="com.mycila" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion>4.0</ignoreVersion>
                <ignoreVersion>4.1</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.jetbrains.kotlin" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion>1.5.0-M1</ignoreVersion>
                <ignoreVersion>1.5.0-M2</ignoreVersion>
                <ignoreVersion>1.5.0-RC</ignoreVersion>
                <ignoreVersion>1.5.0</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="com.github.gantsign.maven" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion>1.6.1</ignoreVersion>
                <ignoreVersion>1.6.0</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>